<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8">
      <title>Connecting</title>
      <link href="../styles/stylesHouse.css" rel="stylesheet" type="text/css"/>
      <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <style>

  </head>
<!-- used https://www.w3schools.com/howto/tryit.asp?filename=tryhow_css_timeline-->
  <body>
    <% include header %>
    <script src="https://unpkg.com/vue@2.5.17/dist/vue.min.js"></script>
    <!-- <img src= alt="Italian Trulli">  -->
    <h1> <%= house.name %> </h1>
    <h2> <%= house.description %> <h2>
    </h3> Events </h3>
    <div class="timeline">
        <% events.forEach(event => { %>
            <div class="container left">
              <div class="content">
                  <h2><%= event.date %></h2>
                  <p><%= event.eventName %>: <%= event.description %> </p>
              </div>
            </div>
        <% }); %>
    </div>
    <h3> Resources </h3>
    <div>
        <ul>
            <% resources.forEach(resource => { %>
                    <% if(resource.titleInfo.title) { %>
                    <li>
                        <%= resource.titleInfo.title %>
                        <% console.log(resource.location) %>
                    </li>
                    <% } %>
            <% }); %>
        </ul>
    </div>
<h3> Featured Rooms </h3>
<div>
    <ul>
        <% featuredRooms.forEach(room => { %>
            <tr>
              <td>
                <a href="/featuredRoom/<%= room.id %>"> <%= room.name %> </a>
              </td>
            </tr>
        <% }); %>
    </ul>
</div>
    <table class="table table-striped">
        <tr><td> Rooms </td></tr>
        <% rooms.forEach(room => { %>
          <tr>
            <td>
              <a href="/room/<%= room.id %>"> <%= room.name %> </a>
            </td>
          </tr>
        <% }); %>
    </table>
  </body>
</html>
